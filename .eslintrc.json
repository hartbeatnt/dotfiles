{
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread":true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "worker":true
    },
    "rules": {
        "semi": 1,
        "no-unused-vars": 1,
        "no-undef": 2
    },
		"globals": {
			"React": true
		}
}
